<% title = t "head.title.db.channels.edit", name: @channel.name %>
<% set_meta_tags(title: title, reverse: true, prefix: false) %>

<div class="container-fluid py-3">
  <div class="mb-3">
    <h1>
      <%= title %>
    </h1>
  </div>

  <div class="mb-3 row">
    <div class="col-6">
      <%= render "form", channel: @channel, form_url: db_channel_detail_path(@channel), form_method: :patch %>
    </div>
  </div>
</div>
